<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Telefono.
    -->
    <changeSet id="20181207095930-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="utenza_telefono_id"
                                 baseTableName="telefono"
                                 constraintName="fk_telefono_utenza_telefono_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utenza"/>

        <addForeignKeyConstraint baseColumnNames="istituto_telefono_id"
                                 baseTableName="telefono"
                                 constraintName="fk_telefono_istituto_telefono_id"
                                 referencedColumnNames="id"
                                 referencedTableName="istituto"/>

    </changeSet>
</databaseChangeLog>
