    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Utenza.
    -->
    <changeSet id="20181031113838-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_utenza_id"
                                 baseTableName="utenza"
                                 constraintName="fk_utenza_user_utenza_id"
