<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Validazione.
    -->
    <changeSet id="20191203115513-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="validazione_telefono_id"
                                 baseTableName="validazione"
                                 constraintName="fk_validazione_validazione_telefono_id"
                                 referencedColumnNames="id"
                                 referencedTableName="telefono"/>

        <addForeignKeyConstraint baseColumnNames="stampa_id"
                                 baseTableName="validazione"
                                 constraintName="fk_validazione_stampa_id"
                                 referencedColumnNames="id"
                                 referencedTableName="storico_telefono"/>

    </changeSet>
</databaseChangeLog>
