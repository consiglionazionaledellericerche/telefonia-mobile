<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Telefonoservizi.
    -->
    <changeSet id="20181207100556-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="telefono_id"
                                 baseTableName="telefonoservizi"
                                 constraintName="fk_telefonoservizi_telefono_id"
                                 referencedColumnNames="id"
                                 referencedTableName="telefono"/>

        <addForeignKeyConstraint baseColumnNames="servizi_id"
                                 baseTableName="telefonoservizi"
                                 constraintName="fk_telefonoservizi_servizi_id"
                                 referencedColumnNames="id"
                                 referencedTableName="servizi"/>

    </changeSet>
</databaseChangeLog>
